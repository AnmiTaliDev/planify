[Application]
name=com.github.alainm23.planner
runtime=io.elementary.Platform/x86_64/6
sdk=io.elementary.Sdk/x86_64/6
command=com.github.alainm23.planner

[Context]
shared=network;ipc;
sockets=x11;wayland;fallback-x11;
devices=dri;

[X-DConf]
migrate-path=/com/github/alainm23/planner/

[Extension com.github.alainm23.planner.Debug]
directory=lib/debug
autodelete=true
no-autodownload=true

[Build]
built-extensions=com.github.alainm23.planner.Debug;